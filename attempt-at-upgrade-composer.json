{
    "name": "llbbl/polr2",
    "description": "The Polr URL Shortener.",
    "keywords": ["url-shortener", "url", "cms"],
    "license": "GPLv2+",
    "type": "project",
    "require": {
        "php": "8.0 || 8.1",
        "laravel/lumen-framework": "v8.3.4",
        "vlucas/phpdotenv": "v5.4.1",
        "yajra/laravel-datatables-oracle": "v9.21.2",
        "paragonie/random_compat": "^v1.4.3",
        "torann/geoip": "3.0.2",
        "geoip2/geoip2": "v2.13.0",
        "nesbot/carbon": "2.62.1",
        "doctrine/dbal": "3.4.2",
        "google/recaptcha": "1.2.4",
        "symfony/http-foundation": "v5.4.12",
        "illuminate/support": "*",
        "illuminate/mail": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.17",
        "symfony/css-selector": "^v3.4.47"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
